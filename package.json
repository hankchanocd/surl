{
  "name": "surl-cli",
  "version": "0.0.0-semantically-released",
  "description": "URL shortener CLI",
  "main": "src/surl.js",
  "scripts": {
    "commit": "git-cz",
    "test": "npm run build; mocha --timeout 6000",
    "test:no-api": "npm run build; NODE_ENV=no-api mocha",
    "watch": "npm test -- -w",
    "prebuild": "rimraf ./build",
    "build": "babel ./src -d ./build",
    "coverage-check": "istanbul cover -x *.test.js _mocha -- -R spec test/test.js",
    "coverage-show": "npm run coverage-check; open coverage/lcov-report/index.html",
    "coverage-report": "./coverage-report.sh",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "bin": {
    "surl": "./build/surl.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hankchanocd/surl-cli.git"
  },
  "keywords": [
    "shortenUrl",
    "surl",
    "url",
    "cli",
    "bit.ly",
    "firebase",
    "goo.gl"
  ],
  "files": [
    "build",
    "README.md"
  ],
  "author": "Hank Chan <hankchanth@icloud.com> (https://github.com/hankchanocd)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hankchanocd/surl-cli/issues"
  },
  "homepage": "https://github.com/hankchanocd/surl-cli#readme",
  "dependencies": {
    "bitly": "^6.0.8",
    "chalk": "^2.4.1",
    "clipboardy": "^1.2.3",
    "cliui": "^4.1.0",
    "columnify": "^1.5.4",
    "commander": "^2.16.0",
    "conf": "^1.4.0",
    "inquirer": "^5.2.0",
    "node-fetch": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.0.4",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "expect.js": "^0.3.1",
    "ghooks": "^2.0.4",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.2",
    "travis-deploy-once": "^5.0.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:no-api"
    }
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
